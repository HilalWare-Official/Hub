	local Players = game:GetService("Players")
	
	local RunService = game:GetService("RunService")
	
	local Workspace = game:GetService("Workspace")
	
	
	
	-- Button (ScreenGui içinde LocalScript)
	
	local button = script.Parent
	
	
	
	-- Toggle durumu
	
	local isActive = false
	
	
	
	-- Structure altındaki parçaları geçirgen hale getiren fonksiyon
	
	local function makeStructureNonCollidable(parent)
	
	    for _, child in ipairs(parent:GetChildren()) do
	
	        if child:IsA("BasePart") then
	
	            child.CanCollide = not isActive -- Toggle durumuna göre CanCollide değişir
	
	        elseif #child:GetChildren() > 0 then
	
	            makeStructureNonCollidable(child)
	
	        end
	
	    end
	
	end
	
	
	
	-- RunService loop
	
	local connection
	
	local function togglePassThrough()
	
	    isActive = not isActive
	
	    
	
	    if isActive then
	
	        -- Açıksa Stepped ile sürekli uygula
	
	        connection = RunService.Stepped:Connect(function()
	
	            if Workspace:FindFirstChild("Structure") then
	
	                makeStructureNonCollidable(Workspace.Structure)
	
	            end
	
	        end)
	
	    else
	
	        -- Kapatınca loop'u durdur ve CanCollide = true yap
	
	        if connection then
	
	            connection:Disconnect()
	
	            connection = nil
	
	        end
	
	        if Workspace:FindFirstChild("Structure") then
	
	            makeStructureNonCollidable(Workspace.Structure)
	
	        end
	
	    end
	
	end
	
	
	
	-- Buton tıklama olayı
	
	button.MouseButton1Click:Connect(togglePassThrough)
	
	
