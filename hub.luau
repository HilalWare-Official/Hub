	        end
	
	        
	
	        -- Karakterin TÜM parçalarını TARA (SADECE burada!)
	
	        for _, part in ipairs(character:GetDescendants()) do
	
	            if part:IsA("BasePart") then
	
	                -- Accessory parçalarını atlamak için
	
	                local isAccessoryPart = false
	
	                local currentParent = part.Parent
	
	                
	
	                -- Parçanın ebeveyn zincirini kontrol et
	
	                while currentParent and currentParent ~= character do
	
	                    if currentParent:IsA("Accessory") then
	
	                        isAccessoryPart = true
	
	                        break
	
	                    end
	
	                    currentParent = currentParent.Parent
	
	                end
	
	                
	
	                -- Accessory parçası değilse durumu uygula
	
	                if not isAccessoryPart then
	
	                    part.CanCollide = not isPassable
	
	                    -- İsteğe bağlı: Transparanlık ekleyebilirsiniz
	
	                    -- if isPassable then
	
	                    --     part.Transparency = 0.5
	
	                    -- else
	
	                    --     part.Transparency = 0
	
	                    -- end
	
	                end
	
	            end
	
	        end
	
	    end
	
	    
	
	    -- 3. Durumu konsola yazdır
	
	    local statusText = isPassable and "AÇIK (içinden geçilebilir)" or "KAPALI (normal)"
	
	
	    
	
	    -- 4. Buton metnini güncelle (isteğe bağlı)
	
	
	end
	
	
	
	-- 5. Yeni oyuncu eklendiğinde, MEVCUT durumu uygula (isteğe bağlı, kaldırılabilir)
	
	local function setupNewPlayer(player)
	
