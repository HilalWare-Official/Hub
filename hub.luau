-- Players.q692q.PlayerGui.HilalWare.Lua.TuT Sürükle.Main.G_2.Araclar.Sekme6.Sekme.Listelemek
local function C_75()
	local script = LMG2L["Listelemek_75"];
	local Players = game:GetService("Players")
	
	local player = Players.LocalPlayer
	
	
	
	local listFrame = script.Parent:WaitForChild("List")
	
	local template = listFrame:WaitForChild("Template")
	
	
	
	-- Cache için tablolar
	
	local toolItems = {}
	
	local activeItems = {}
	
	
	
	-- Tool içindeki Script sayısı
	
	local function countScripts(tool)
	
		local count = 0
	
		for _, obj in ipairs(tool:GetDescendants()) do
	
			if obj:IsA("Script") or obj:IsA("LocalScript") then
	
				count += 1
	
			end
	
		end
	
		return count
	
	end
	
	
	
	-- Workspace içindeki Tool'lar için Handle bul (sadece ihtiyaç duyulduğunda)
	
	local function getHandle(tool)
	
		for _, obj in ipairs(tool:GetDescendants()) do
	
			if obj.Name == "Handle" and obj:IsA("BasePart") then
	
				return obj
	
			end
	
		end
	
		return nil
	
	end
	
	
	
	-- Tool list item oluştur
	
	local function createItem(tool)
	
		if activeItems[tool] then
	
			return activeItems[tool]
	
		end
	
		
	
		local clone = template:Clone()
	
		clone.Visible = true
	
		clone.Parent = listFrame
	
	
	
		clone.ToolName.Text = tool.Name
	
		clone.Quantity.Text = tostring(countScripts(tool))
	
	
	
		-- EQUIP: Handle'ı oyuncunun pozisyonuna taşır
	
		clone.Equip.MouseButton1Click:Connect(function()
	
			local handle = getHandle(tool)
	
			local char = player.Character or player.CharacterAdded:Wait()
	
			local hrp = char:FindFirstChild("HumanoidRootPart")
	
			if handle and hrp then
	
				handle.CFrame = CFrame.new(hrp.Position)
	
			end
	
		end)
	
	
	
		-- DELETE: Workspace içindeki Tool'u siler
	
		clone.Delete.MouseButton1Click:Connect(function()
	
			if tool and tool.Parent == workspace then
	
				tool:Destroy()
	
			end
	
			if activeItems[tool] then
	
				activeItems[tool]:Destroy()
	
				activeItems[tool] = nil
	
			end
	
		end)
	
	
	
		activeItems[tool] = clone
	
		return clone
	
	end
	
	
	
	-- İlk başta tüm Tool'ları bul
	
	local function initialScan()
	
		for _, obj in ipairs(workspace:GetChildren()) do
	
			if obj:IsA("Tool") then
	
				createItem(obj)
	
			end
	
		end
	
	end
	
	
	
	-- Event tabanlı güncelleme
	
	local function setupToolEvents()
	
		-- Yeni Tool eklendiğinde
	
		workspace.ChildAdded:Connect(function(child)
	
			task.wait(0.1) -- Tool'un tam olarak yüklenmesini bekle
	
			if child:IsA("Tool") then
	
				createItem(child)
	
			end
	
		end)
	
		
	
		-- Tool kaldırıldığında
	
		workspace.ChildRemoved:Connect(function(child)
	
			if child:IsA("Tool") and activeItems[child] then
	
				activeItems[child]:Destroy()
	
				activeItems[child] = nil
	
			end
	
		end)
	
		
	
		-- Tool içindeki değişiklikleri takip et
	
		for _, tool in ipairs(workspace:GetChildren()) do
	
			if tool:IsA("Tool") then
	
				-- Tool'un Parent'i değişirse (alınırsa vs.)
	
				tool:GetPropertyChangedSignal("Parent"):Connect(function()
	
					task.wait(0.1)
	
					if tool.Parent ~= workspace and activeItems[tool] then
	
						activeItems[tool]:Destroy()
	
						activeItems[tool] = nil
	
					elseif tool.Parent == workspace and not activeItems[tool] then
	
						createItem(tool)
	
					end
	
				end)
	
			end
	
		end
	
	end
	
	
	
	-- UI güncellemelerini throttle et
	
	local function updateUIThrottled()
	
		-- Sadece aktif Tool'ları güncelle
	
		for tool, item in pairs(activeItems) do
	
			if tool and tool.Parent == workspace then
	
				item.ToolName.Text = tool.Name
	
				item.Quantity.Text = tostring(countScripts(tool))
	
			else
	
				-- Tool artık workspace'te değilse item'ı kaldır
	
				item:Destroy()
	
				activeItems[tool] = nil
	
			end
	
		end
	
	end
	
	
	
	-- Başlangıç işlemleri
	
	initialScan()
	
	setupToolEvents()
	
	
	
	-- UI güncellemesini throttle et (her 2 saniyede bir)
	
	task.spawn(function()
	
		while true do
	
			updateUIThrottled()
	
			task.wait(2) -- 0.5 yerine 2 saniye
	
		end
	
	end)	
end;
task.spawn(C_75);
